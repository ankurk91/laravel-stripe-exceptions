{
  "name": "ankurk91/laravel-stripe-exceptions",
  "description": "Handle Stripe exceptions gracefully in Laravel php framework.",
  "keywords": [
    "laravel",
    "stripe",
    "exception"
  ],
  "homepage": "https://github.com/ankurk91/laravel-stripe-exceptions",
  "license": "MIT",
  "authors": [
    {
      "name": "ankurk91",
      "homepage": "https://ankurk91.github.io"
    }
  ],
  "require": {
    "php": "^8.0.0",
    "illuminate/http": "^9.34 || ^10",
    "illuminate/support": "^9.34|| ^10",
    "illuminate/translation": "^9.34 || ^10",
    "stripe/stripe-php": "^8.0 || ^9.0 || ^10.0 || ^11.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^7.9 || ^8.0",
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "Ankurk91\\StripeExceptions\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ankurk91\\StripeExceptions\\StripeServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ankurk91\\StripeExceptions\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:coverage": "vendor/bin/phpunit --coverage-clover=coverage.xml"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
