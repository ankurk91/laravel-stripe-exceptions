{
  "name": "ankurk91/laravel-stripe-exceptions",
  "description": "Handle Stripe exceptions gracefully in Laravel php framework.",
  "keywords": [
    "laravel",
    "stripe",
    "exception"
  ],
  "homepage": "https://github.com/ankurk91/laravel-stripe-exceptions",
  "license": "MIT",
  "authors": [
    {
      "name": "ankurk91",
      "homepage": "https://ankurk91.github.io"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "stripe/stripe-php": "^6.15",
    "illuminate/support": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Ankurk91\\StripeExceptions\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ankurk91\\StripeExceptions\\StripeServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
